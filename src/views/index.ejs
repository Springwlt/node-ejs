<%- include header.ejs %>
<body>
<h1  class="content">用户列表</h1>
<div class="box-body">
    <table class="table table-bordered table-hover">
        <thead>
        <tr>
            <th width="150px">用户名</th>
            <th width="150px">年龄</th>
            <th width="300px">性别</th>
        </tr>
        </thead>
        <tbody>
        <% datas.forEach(function (datas) { %>
        <tr>
            <td>
               <%=datas.name %>
            </td>
            <td>
                <%=datas.age %>
            </td>
            <td>
                <%=datas.sex %>
            </td>
        </tr>
        <% }) %>
        </tbody>
        <tfoot>
        </tfoot>
    </table>
</div>
</body>
</html>